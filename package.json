{
  "name": "flatlock",
  "version": "0.0.0",
  "description": "The Matlock of lockfile parsers - extracts packages without building dependency graphs",
  "keywords": [
    "lockfile",
    "lockfiles",
    "shrinkwrap",
    "npm",
    "pnpm",
    "yarn",
    "yarn-berry"
  ],
  "homepage": "https://github.com/user/flatlock#readme",
  "bugs": {
    "url": "https://github.com/user/flatlock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/flatlock.git"
  },
  "license": "Apache-2.0",
  "author": "Charlie Robbins <npm@charlie.dev>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "main": "src/index.js",
  "bin": {
    "lockcmp": "./bin/lockcmp.js"
  },
  "files": [
    "src",
    "dist",
    "bin"
  ],
  "scripts": {
    "test": "node --test ./test/*.test.js",
    "test:coverage": "c8 node --test ./test/*.test.js",
    "typecheck": "tsc --noEmit",
    "build:types": "tsc",
    "lint": "biome lint src test",
    "lint:fix": "biome lint --write src test",
    "lint:md": "markdownlint-cli2 \"README.md\"",
    "format": "biome format --write src test",
    "format:check": "biome format src test",
    "check": "biome check src test",
    "check:fix": "biome check --write src test",
    "prepublishOnly": "npm run build:types"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "@yarnpkg/parsers": "^3.0.3",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@npmcli/arborist": "^9.1.9",
    "@pnpm/lockfile-file": "^9.0.0",
    "c8": "^10.1.3",
    "markdownlint-cli2": "^0.17.2",
    "snyk-nodejs-lockfile-parser": "^1.55.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  },
  "c8": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ]
  }
}
